{
   "background": {
      "scripts": [ "extension/MTDBackground.js" ]
   },
   "browser_action": {
      "default_icon": "sources/oneclick/InstaTweetDeck.png",
      "default_popup": "extension/ITD.html",
      "default_title": "Launch TweetDeck"
   },
   "content_scripts": [ {
      "css": [ "sources/dev.css" ],
      "js": [ "extension/MTDLoad.js" ],
      "matches": [ "https://tweetdeck.twitter.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "TweetDeck, just more beautiful, colourful, customisable, fast, and fun than ever.",
   "icons": {
      "16": "AppIcon16.png",
      "24": "AppIcon24.png",
      "32": "AppIcon32.png",
      "48": "AppIcon48.png",
      "64": "AppIcon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ModernDeck",
   "permissions": [ "webRequest", "webRequestBlocking", "https://*.twitter.com/", "https://ton.twimg.com/", "contextMenus", "tabs", "storage" ],
   "short_name": "ModernDeck",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "6.2",
   "web_accessible_resources": [ "sources", "sources/*", "sources/cssextensions/*", "sources/fonts/*", "sources/libraries/*" ]
}
