{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon19.png",
      "default_popup": "popup.html",
      "default_title": "Sci-hub Links"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "style.css" ],
      "exclude_globs": [ "*://*.google.*/*", "*://*.bing.*/*", "*://*twitter.com/*" ],
      "js": [ "jquery.min.js", "jquery.ba-replacetext.js", "scihub_links.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_main_description__",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Sci-hub Links",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "storage", "contextMenus" ],
   "short_name": "Sci-hub Links",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.2"
}
